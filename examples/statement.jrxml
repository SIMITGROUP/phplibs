    <queryString>
        <![CDATA[SELECT *,@balance:=ROUND(@balance+COALESCE(r.balance, 0.00), 2) as balanceamt from select(b.* ,bp.bpartner_no,i.document_no as knockoffdoc,i.document_date as knockoffdoc_date
                ,case when i.document_no is null then 'bal' 
                else
                    'knockoff' 
                end as grouping,
                concat(
                    CASE WHEN addr.address_street1!='' THEN concat(addr.address_street1,'
') else '' end ,
                    CASE WHEN addr.address_street2!='' THEN concat(addr.address_street2,'
') else '' end ,
                    CASE WHEN addr.address_street3!='' THEN concat(addr.address_street3,'
') else '' end ,
                    CASE WHEN addr.address_street4!='' THEN concat(addr.address_street4,'
') else '' end ,
                    addr.address_postcode,' ',addr.address_city ,' ',area.area_name,
                    CONCAT('
', getCountryName(addr.address_country))) as bpartneraddress,
                    concat(c.greeting,' ',c.contacts_name) AS contactperson,
                    b.amount-b.process_amt as balance,
                    b.amount-b.process_amt as balance2
            FROM 
            (
              SELECT m.*, 'S' AS type, 
                CASE WHEN sum(m.trans_amt)<0 THEN 
                        SUM(m.trans_amt)*-1 
                    ELSE 
                        SUM(m.trans_amt) 
                END AS amount,
                CASE WHEN  SUM(m.trans_amt_local)<0 THEN 
                        SUM(m.trans_amt_local)*-1 
                    ELSE 
                        SUM(m.trans_amt_local) 
                END AS amount_local,
                CASE WHEN m.document_type='JE' THEN 
                        getKnockoffProcessAmt(CONCAT(m.document_type,'|',m.documentline_id),1,0,m.currency_code,'MYR') 
                    ELSE 
                CASE WHEN m.document_type in ('PI','SI','ARDN','APCN')
                THEN
                        getKnockoffProcessAmt(CONCAT(m.document_type,'|',m.document_id),0,0,m.currency_code,'MYR') 
                ELSE  
                        getKnockoffProcessAmt(CONCAT(m.document_type,'|',m.document_id),1,0,m.currency_code,'MYR') 
                END
                END AS process_amt, 
                0 AS process_amt_local,
                CASE WHEN m.document_type='JE' THEN 
                        CONCAT(m.document_type,'|',m.documentline_id) 
                    ELSE 
                        CONCAT(m.document_type,'|',m.document_id) 
                END AS id,
                '' as manual_amt ,a.accounts_type
                FROM (
                SELECT d.document_id,0 AS documentline_id,d.document_no,
            d.document_refno,d.document_date,d.accounts_id,
            d.document_type,
            CASE WHEN acc.accounts_type IN ('Debtors','Creditors') THEN d.bpartner_id ELSE '' END AS bpartner_id,
            if(d.paidfrom_to!='', d.paidfrom_to,bp.bpartner_name) as bpartner_name,
            '' AS terms_id,0 AS terms_day,d.currency_code,
            d.currency_rate,d.document_bankrefno,d.payment_method,
            CASE WHEN d.document_type IN ('RC','APDN','CB') THEN d.totalnetamt ELSE d.totalnetamt*-1 END AS trans_amt,
            CASE WHEN d.document_type IN ('RC','APDN','CB') THEN d.totalnetamt_local ELSE d.totalnetamt_local*-1 END AS trans_amt_local,
            '' AS item_id, '' AS item_code,
            CASE WHEN d.document_type IN ('RC','APDN','CB') THEN 
                CONCAT('Received from ',d.paidfrom_to)
                ELSE CONCAT('Paid to ',d.paidfrom_to) END AS item_name,
            getDocTypeName(d.document_type) AS documentline_desc,
            d.track1_id,d.track2_id,d.track3_id,d.track4_id,d.track5_id,
            d.organization_id,d.branch_id, 
            '' AS batchno, '' AS lotno,d.salesagent_id,
            '' AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_payment d
            LEFT JOIN acc_accounts acc ON acc.accounts_id=d.accounts_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id
            WHERE d.document_status = 'CO' AND d.organization_id=1 
              AND (d.document_type!='CB' OR d.accounts_id!='')
            AND d.totalnetamt != 0  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
            
                         UNION ALL
                         SELECT d.document_id,dl.documentline_id,d.document_no,d.document_refno,d.document_date,
            dl.accounts_id,
            d.document_type,
            CASE WHEN acc.accounts_type IN ('Debtors','Creditors') THEN CASE WHEN dl.bpartner_id ='' THEN d.bpartner_id ELSE dl.bpartner_id END  ELSE '' END AS bpartner_id,
            if(d.paidfrom_to!='', d.paidfrom_to,bp.bpartner_name) AS bpartner_name,'' AS terms_id,0 AS terms_day,
            d.currency_code,d.currency_rate,d.document_bankrefno,
            d.payment_method,
            CASE WHEN d.document_type IN ('RC','APDN','CB') THEN dl.amount_sub*-1 ELSE dl.amount_sub END AS trans_amt,
            CASE WHEN d.document_type IN ('RC','APDN','CB') THEN dl.amount_sub_local*-1 ELSE dl.amount_sub_local END AS trans_amt_local,
            dl.item_id, dl.item_code,dl.item_name,dl.item_name as documentline_desc,
            dl.track1_id,dl.track2_id,dl.track3_id,dl.track4_id,dl.track5_id,
            d.organization_id,d.branch_id,
            dl.batchno, dl.lotno,d.salesagent_id,
            dl.tax_id AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_payment d
            INNER JOIN acc_paymentline dl ON dl.document_id=d.document_id
            LEFT JOIN acc_accounts acc ON acc.accounts_id=dl.accounts_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            LEFT JOIN acc_track tr1 ON tr1.track_id=dl.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=dl.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=dl.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=dl.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=dl.track5_id

            WHERE d.document_status = 'CO' AND d.organization_id=1 
             AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
                         UNION ALL
                         
            SELECT d.document_id,CONCAT(d.document_id,'ROUND'),d.document_no,
            d.document_refno,d.document_date,d.rounding_account_id,
            d.document_type,'' AS bpartner_id,if(d.paidfrom_to!='', d.paidfrom_to,bp.bpartner_name) AS bpartner_name,
            '' AS terms_id,0 AS terms_day,
            'MYR' as currency_code,1 AS currency_rate,d.document_bankrefno,d.payment_method,
            CASE WHEN d.document_type IN ('RC','ARCN') THEN d.rounding_amount*-1 ELSE d.rounding_amount END AS trans_amt,
            CASE WHEN d.document_type IN ('RC','ARCN') THEN d.rounding_amount*-1 ELSE d.rounding_amount END AS trans_amt_local,
            '' AS item_id, '' AS item_code,'Rounding Variance' AS item_name,getDocTypeName(d.document_type) AS documentline_desc,
            '' AS track1_id,'' AS track2_id,'' AS track3_id,'' AS track4_id,'' AS track5_id,
            d.organization_id,d.branch_id,
            '' AS batchno, '' AS lotno,d.salesagent_id,
            '' AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code   
            FROM acc_payment d
            INNER JOIN acc_accounts acc on acc.accounts_id=d.rounding_account_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id
            WHERE d.document_status = 'CO' AND d.organization_id=1 
            AND d.rounding_amount!=0  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
                         UNION ALL
                                    
            SELECT d.document_id,CONCAT(dl.documentline_id,'EGL-AC'),d.document_no,
            d.document_refno,d.document_date,d.exchangegainloss_account_id,
            d.document_type,d.bpartner_id,d.paidfrom_to AS bpartner_name,
            '' AS terms_id,0 AS terms_day,
            'MYR' as currency_code,1 AS currency_rate,d.document_bankrefno,
            d.payment_method,
            CASE WHEN d.document_type IN ('RC','CB','ARCN') THEN SUM(ko.exchangegainloss_amount)*-1 ELSE SUM(ko.exchangegainloss_amount) END AS trans_amt,
            CASE WHEN d.document_type IN ('RC','CB','ARCN') THEN SUM(ko.exchangegainloss_amount)*-1 ELSE SUM(ko.exchangegainloss_amount) END AS trans_amt_local,
            '' AS item_id, '' AS item_code,'Exchange Gain/Loss' AS item_name,getDocTypeName(d.document_type) AS documentline_desc,
            d.track1_id,d.track2_id,d.track3_id,d.track4_id,d.track5_id,
            d.organization_id,d.branch_id,
            dl.batchno, dl.lotno,d.salesagent_id,
            d.exchangegainloss_tax_id AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_paymentline_knockoff ko
            INNER JOIN acc_paymentline dl ON dl.documentline_id=ko.paymentline_id
            INNER JOIN acc_payment d ON d.document_id=dl.document_id
            INNER JOIN acc_accounts acc on acc.accounts_id=d.exchangegainloss_account_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id
            WHERE d.document_status='CO' AND d.organization_id=1 
            AND d.isautoexchangegainloss=1
            AND ko.exchangegainloss_amount!=0  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
            GROUP BY d.document_id
            HAVING trans_amt!=0

            UNION ALL

            SELECT d.document_id,CONCAT(dl.documentline_id,'EGL-BP'),d.document_no,
            d.document_refno,d.document_date, IF(acc.accounts_type IN ('Debtors','Creditors') , d.bpartneraccounts_id ,dl.bpartneraccounts_id) as accounts_id,
            d.document_type,d.bpartner_id,d.paidfrom_to AS bpartner_name,
            '' AS terms_id,0 AS terms_day,
            'MYR' as currency_code,1 AS currency_rate,d.document_bankrefno,
            d.payment_method,
            CASE WHEN d.document_type IN ('RC','CB','ARCN') THEN SUM(ko.exchangegainloss_amount) ELSE SUM(ko.exchangegainloss_amount)*-1 END AS trans_amt,
            CASE WHEN d.document_type IN ('RC','CB','ARCN') THEN SUM(ko.exchangegainloss_amount) ELSE SUM(ko.exchangegainloss_amount)*-1 END AS trans_amt_local,
            '' AS item_id, '' AS item_code,'Exchange Gain/Loss' AS item_name,getDocTypeName(d.document_type) AS documentline_desc,
            d.track1_id,d.track2_id,d.track3_id,d.track4_id,d.track5_id,
            d.organization_id,d.branch_id,
            dl.batchno, dl.lotno,d.salesagent_id,
            dl.tax_id AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_paymentline_knockoff ko
            INNER JOIN acc_paymentline dl ON dl.documentline_id=ko.paymentline_id
            INNER JOIN acc_payment d ON d.document_id=dl.document_id            
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            INNER JOIN acc_accounts acc ON acc.accounts_id=d.bpartneraccounts_id    
            LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id     
            WHERE d.document_status='CO' AND d.organization_id=1 
            AND d.isautoexchangegainloss=1
            AND ko.exchangegainloss_amount!=0  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
            GROUP BY d.document_id
            HAVING trans_amt!=0
                         UNION ALL
                         SELECT d.document_id,0 AS documentline_id,d.document_no,
            d.document_refno,d.document_date,d.bpartneraccounts_id AS accounts_id,
            d.document_type,d.bpartner_id,d.bpartner_name,d.terms_id,d.terms_day,
            d.currency_code,d.currency_rate,'' AS document_bankrefno,
            '' as payment_method,
            CASE WHEN d.document_type IN ('SI','ARDN') THEN d.totalnetamt ELSE d.totalnetamt*-1 END AS trans_amt,
            CASE WHEN d.document_type IN ('SI','ARDN') THEN d.totalnetamt_local ELSE d.totalnetamt_local*-1 END AS trans_amt_local,
            '' AS item_id, '' AS item_code,'' AS item_name,getDocTypeName(d.document_type) AS documentline_desc,
            d.track1_id,d.track2_id,d.track3_id,d.track4_id,d.track5_id,
            d.organization_id,d.branch_id,
            '' AS batchno, '' AS lotno,d.salesagent_id,
            '' AS tax_id,'' AS prepaidknockoff_exchange_refer_id,'' AS prepaid_processid,d.updated,d.updatedby,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_invoice d
            INNER JOIN acc_accounts acc on acc.accounts_id=d.bpartneraccounts_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
            LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id 
            WHERE d.document_status = 'CO'  AND d.organization_id=1 
            AND d.totalnetamt != 0  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
                         UNION ALL
                         SELECT d.document_id,dl.documentline_id,
            case WHEN dl.ref_no='' THEN d.document_no else dl.ref_no end as document_no,
            d.document_title AS document_refno,d.document_date,dl.accounts_id,
            d.document_type AS document_type,dl.bpartner_id AS bpartner_id,'' AS bpartner_name,
            dl.terms_id AS terms_id,t.terms_day,
            dl.currency_code,dl.currency_rate,dl.chequeno AS document_bankrefno,
            '' AS payment_method,
            amt AS trans_amt,
            amt_local AS trans_amt_local,
            '' AS item_id, '' AS item_code,dl.memo AS item_name,dl.memo AS documentline_desc,
            dl.track1_id,dl.track2_id,dl.track3_id,dl.track4_id,dl.track5_id,
            d.organization_id,d.branch_id,
            '' AS batchno, '' AS lotno,0 AS salesagent_id,
            dl.tax_id AS tax_id,dl.prepaidknockoff_exchange_refer_id,d.prepaid_processid,d.updated,d.updatedby
            ,
            tr1.track_code AS track1_code,tr2.track_code AS track2_code,tr3.track_code AS track3_code,tr4.track_code AS track4_code,tr5.track_code AS track5_code
            FROM acc_journalentry d
            INNER JOIN acc_journalentryline dl ON dl.document_id=d.document_id
            INNER JOIN acc_accounts acc on acc.accounts_id=dl.accounts_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=dl.bpartner_id
            LEFT JOIN acc_terms t ON t.terms_id=dl.terms_id         
            LEFT JOIN acc_track tr1 ON tr1.track_id=dl.track1_id
            LEFT JOIN acc_track tr2 ON tr2.track_id=dl.track2_id
            LEFT JOIN acc_track tr3 ON tr3.track_id=dl.track3_id
            LEFT JOIN acc_track tr4 ON tr4.track_id=dl.track4_id
            LEFT JOIN acc_track tr5 ON tr5.track_id=dl.track5_id
            WHERE d.document_status='CO'  AND d.organization_id=1  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors'
                         UNION ALL
                         SELECT d.openingbalanceline_id,d.documentline_id,d.document_no,d.document_refno,d.document_date,d.accounts_id,
            d.document_type, d.bpartner_id,d.bpartner_name,d.terms_id,d.terms_day,d.currency_code,d.currency_rate,
            d.document_bankrefno,d.payment_method,d.trans_amt,d.trans_amt_local,d.item_id,d.item_code,d.item_name,
            d.documentline_desc, d.track1_id,d.track2_id,d.track3_id,d.track4_id,d.track5_id,d.organization_id,
                d.branch_id,
           d.batchno,d.lotno,d.salesagent_id,d.tax_id,'' AS prepaidknockoff_exchange_refer_id,
           '' AS prepaid_processid ,d.updated,d.updatedby as updatedby,
           '' AS track1_code,'' AS track2_code,'' AS track3_code,'' AS track4_code,'' AS track5_code
        FROM view_acc_openingbalance d      
        INNER JOIN acc_accounts acc on acc.accounts_id=d.accounts_id
        LEFT JOIN bp_bpartner bp on bp.bpartner_id=d.bpartner_id
        LEFT JOIN acc_track tr1 ON tr1.track_id=d.track1_id
        LEFT JOIN acc_track tr2 ON tr2.track_id=d.track2_id
        LEFT JOIN acc_track tr3 ON tr3.track_id=d.track3_id
        LEFT JOIN acc_track tr4 ON tr4.track_id=d.track4_id
        LEFT JOIN acc_track tr5 ON tr5.track_id=d.track5_id
         WHERE d.organization_id=1  AND d.document_date BETWEEN '0000-00-00' AND '9999-12-31' AND bp.bpartner_id = '57' AND acc.accounts_type = 'Debtors' 
                ) m
                LEFT JOIN acc_accounts a ON a.accounts_id = m.accounts_id
                WHERE a.accounts_type='Debtors'
                AND m.prepaidknockoff_exchange_refer_id=''
                AND m.prepaid_processid=''
                AND m.documentline_id NOT LIKE '%EGL-BP' 
                AND m.organization_id=1 
                AND m.document_date between '0000-00-00' AND '9999-12-31'
                 AND m.bpartner_id ='57'    
                GROUP BY CASE WHEN document_type='JE' THEN m.documentline_id ELSE m.document_id END,m.document_type,m.document_no
            )b
            LEFT JOIN acc_knockoff kf on kf.from_doc_id =b.id
            LEFT JOIN acc_invoice i on concat(i.document_type,'|',i.document_id)=kf.to_doc_id
            LEFT JOIN acc_journalentryline jel on concat('JE|',jel.documentline_id)=kf.to_doc_id
            LEFT JOIN acc_journalentry je on je.document_id=jel.document_id
            LEFT JOIN bp_bpartner bp on bp.bpartner_id=b.bpartner_id
            LEFT JOIN bp_address addr on addr.bpartner_id=bp.bpartner_id and addr.isactive=1
            LEFT JOIN (SELECT * FROM bp_contacts c where c.bpartner_id='57'  and c.isactive=1 limit 1 ) c on c.bpartner_id=bp.bpartner_id
            LEFT JOIN bp_area area on addr.address_area=area.area_id
            WHERE b.document_id != '' 
             AND b.bpartner_id ='57' 
            AND (
                 (b.document_type in ('SI','PI','APCN','ARDN') and b.process_amt-b.amount!=0) OR
                 (b.document_type in ('RC','PV','APDN','ARCN'))
                )
  order by knockoffdoc asc)r,
  (SELECT @balance:=0)bb]]>
    </queryString>
    <field name="balanceamt" class="java.lang.String">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="document_id" class="java.lang.String">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="documentline_id" class="java.lang.String">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="document_no" class="java.lang.String">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="document_refno" class="java.lang.String">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="document_date" class="java.sql.Date">
        <fieldDescription><![CDATA[]]></fieldDescription>
    </field>
    <field name="accounts_id" class="java.lang.String">
