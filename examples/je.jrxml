	<queryString>
		<![CDATA[SELECT 
do.*,dol.*,acc.accounts_name,acc.accounts_code,tax.tax_code,dol.bpartner_id,
IF(dol.amt_local >0, dol.amt_local,0) as debitamt_local,
					IF(dol.amt_local <0, dol.amt_local * -1,0) as creditamt_local,
					IF(dol.amt >0, dol.amt,0) as debitamt,
					IF(dol.amt <0, dol.amt * -1,0) as creditamt
FROM acc_journalentry do
left join acc_journalentryline dol on do.document_id=dol.document_id
left join acc_accounts acc on dol.accounts_id=acc.accounts_id
left join acc_tax tax on tax.tax_id=dol.tax_id
left join bp_bpartner bp on bp.bpartner_id = dol.bpartner_id
left join bp_address ad on ad.bpartner_id = dol.bpartner_id
WHERE do.organization_id = 1 and do.document_id =$P{record_id}]]>
	</queryString>
